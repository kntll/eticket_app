<%= form_for(@micropost) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :name_ticket %>
    <%= f.text_field :name_ticket %>


    <%= f.text_area :content, placeholder: "Upload a eticket" %> 
  </div>
  <%= f.submit "Sell", class: "btn btn-large btn-primary" %>
<% end %>


